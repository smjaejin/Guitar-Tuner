        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageViewTick300"
        app:srcCompat="@drawable/rectanlge" />

    <ImageView
        android:id="@+id/imageViewTick300"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewGreen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageViewGreen"
        app:srcCompat="@drawable/rectanlge" />

    <ImageView
        android:id="@+id/imageViewRedBot2"

<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    android:visibility="visible"
    tools:context="com.seanmclane.guitar_tuner.MainActivity">


    <TextView
        android:id="@+id/textViewNote"
        android:layout_width="96sp"
        android:layout_height="96sp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/white"
        android:text="E"
        android:textAlignment="center"
        android:textAppearance="@android:style/TextAppearance.Widget.Toolbar.Title"
        android:textSize="72sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.35" />

    <ImageView
        android:id="@+id/imageViewYello"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:background="#fff000"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewTick060"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageViewGreen"
        app:layout_constraintTop_toTopOf="@+id/imageViewTick060"
        app:srcCompat="@drawable/rectanlge2" />

    <ImageView
        android:id="@+id/imageViewRed"
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#ff0000"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewTick060"
        app:layout_constraintEnd_toStartOf="@+id/imageViewGreen"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageViewTick060"
        app:srcCompat="@drawable/rectanlge2" />

    <ImageView
        android:id="@+id/imageViewGreen"
        android:layout_width="30dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:background="#00ff00"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageViewYello"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageViewRed"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.74"
        app:srcCompat="@drawable/rectanlge2" />

    <ImageView
        android:id="@+id/imageViewRedBot"
        android:layout_width="150dp"
        android:layout_height="22dp"
        android:background="#ff0000"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewRed"
        app:layout_constraintStart_toStartOf="@+id/imageViewRed"
        app:srcCompat="@drawable/rectanlge2" />

    <ImageView
        android:id="@+id/imageViewYellowBot"
        android:layout_width="150dp"
        android:layout_height="22dp"
        android:background="#fff000"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewYello"
        app:layout_constraintEnd_toEndOf="@+id/imageViewYello"
        app:srcCompat="@drawable/rectanlge2" />

    <ImageView
        android:id="@+id/imageViewTick060"
        android:layout_width="60dp"
        android:layout_height="45dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewTick300"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageViewTick300"
        app:srcCompat="@drawable/rectanlge" />

    <ImageView
